{
  "name": "sapper-typescript-graphql-template",
  "description": "A template that includes Svelte with Sapper, TypeScript preprocessing, and a GraphQL server through TypeGraphQL",
  "keywords": [
    "sapper",
    "typegraphql",
    "typescript",
    "eslint",
    "svelte",
    "apollo-server",
    "graphql"
  ],
  "homepage": "https://github.com/babichjacob/sapper-typescript-graphql-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/babichjacob/sapper-typescript-graphql-template.git"
  },
  "license": "MIT",
  "version": "2020.10.21",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.ts ./src/components/**/*.svelte ./src/graphql/**/*.ts ./src/routes/**/*.svelte ./src/routes/**/*.ts",
    "validate": "svelte-check --ignore src/node_modules/@svazzle",
    "validate:dev": "svelte-check --ignore src/node_modules/@svazzle --watch",
    "sapper:dev": "svazzle dev",
    "sapper:build": "cross-env NODE_ENV=production svazzle build",
    "sapper:export": "cross-env NODE_ENV=production svazzle export --legacy",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 run-p validate:dev sapper:dev",
    "prod": "cross-env NODE_OPTIONS=--max_old_space_size=4096 run-s sapper:build validate",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 node __sapper__/build",
    "export": "cross-env NODE_OPTIONS=--max_old_space_size=4096 run-s sapper:export validate",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@mikro-orm/cli": "^4.2.2",
    "@mikro-orm/core": "^4.2.2",
    "@mikro-orm/migrations": "^4.2.2",
    "@mikro-orm/postgresql": "^4.2.2",
    "@mikro-orm/reflection": "^4.2.2",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "apollo-server-express": "^2.19.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-fetch": "^2.6.1",
    "secure-password": "^4.0.0",
    "sirv": "^1.0.9",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/node-fetch": "^2.5.7",
    "@types/secure-password": "^3.1.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "bufferutil": "^4.0.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "graphql": "^15.4.0",
    "graphql-codegen-svelte": "0.0.7",
    "kleur": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "postcss-import": "^13.0.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.34.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.31.0",
    "svelte-apollo": "^0.4.0",
    "svelte-check": "^1.1.19",
    "svelte-forms-lib": "^1.2.2",
    "svelte-preprocess": "^4.6.1",
    "svelte-typed-component": "^1.1.1",
    "tailwindcss": "^2.0.1",
    "ts-node": "^9.1.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "utf-8-validate": "^5.0.3"
  },
  "resolutions": {
    "**/**/fs-capacitor": "^6.2.0"
  },
  "mikro-orm": {
    "configPaths": [
      "./src/backend/mikro-orm.config.ts"
    ]
  }
}
